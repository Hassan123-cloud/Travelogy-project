  "scripts": {
    "setLocalEnv": "cross-env DOTENV_CONFIG_PATH=./.github/envs/.env node -r dotenv/config .github/scripts/setRuntimeEnv.js public/",
    "setLocalEnvNoAuth": "cross-env DOTENV_CONFIG_PATH=./.github/envs/.env-noauth node -r dotenv/config .github/scripts/setRuntimeEnv.js public/",
    "start": "npm run setLocalEnv && react-scripts start",
    "start:dev": "concurrently \"npm run setLocalEnv\" \"cross-env PORT=3000 REACT_APP_AUTH=NO_AUTH react-scripts start\"",
    "start:local": "concurrently \"launch-luminate-portal\" \"cross-env BROWSER=none react-scripts start\"",
    "start:local:noauth": "concurrently \"node mock\" \"npm run setLocalEnvNoAuth\" \"cross-env PORT=3000 react-scripts start\"",
    "build": "react-scripts --max_old_space_size=6144 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test-coverage": "react-scripts test --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "prettier": "prettier --check src/**/*.ts src/**/*.tsx",
    "format": "prettier --write src/**/*.ts src/**/*.tsx",
    "pre-commit": "concurrently \"yarn format\" \"npm run lint\"",
    "test:e2e:local:chrome:devtools": "TZ=America/Chicago TS_NODE_PROJECT=tsconfig.e2e.json wdio run e2e/configs/wdio.local.chrome.devtools.conf.ts",
    "test:e2e:chrome": "cross-env TZ=America/Chicago TS_NODE_PROJECT=e2e/tsconfig.json wdio run e2e/configs/wdio.local.chrome.conf.ts",
    "test:e2e:saucelabs": "cross-env TZ=America/Chicago TS_NODE_PROJECT=e2e/tsconfig.json wdio run e2e/configs/wdio.sauce.conf.ts",
    "test:e2e:saucelabs:availability": "cross-env TZ=America/Chicago TS_NODE_PROJECT=e2e/tsconfig.json  wdio run  e2e/configs/wdio.sauce.availability.conf.ts -l debug"
  },
  "eslintConfig": {
    "extends": "react-app"
